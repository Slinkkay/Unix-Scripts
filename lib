writeItem(){
  echo "$1='$2'" >> $3
}

writeProperties(){
  keys=$1
  values=$2
  size=$3
  rm $4 
  for ((i=0; i < size; i++))
  do
    writeItem "${keys[i]}" "${values[i]}" "$4"
  done
}

echoProperties(){
  keys=$1
  values=$2
  size=$3;
  for ((i=0; i< size; i++)) 
  do  
    echo "${keys[i]}":  "${values[i]}"
  done
}

